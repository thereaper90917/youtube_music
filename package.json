{
  "name": "youtube-music",
  "productName": "youtube-music",
  "version": "0.0.12",
  "description": "My Electron application description",
  "repository": "https://github.com/thereaper90917/youtube_music",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "thereaper90917",
    "email": "viper90917@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./ico/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "youtube_music",
            "loadingGif": "./ico/loading.gif",
            "setupIcon": "./ico/icon.ico",
            "iconUrl": "https://cdn0.iconfinder.com/data/icons/brands-colored-2/232/music-social-network-brand-logo-256.png"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "thereaper90917",
              "name": "youtube_music",
              "draft": true
            }
          }
        }
      ]
    }
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.20.3",
    "cross-fetch": "^3.0.6",
    "discord-rpc": "^3.2.0",
    "electron-log": "^4.3.2",
    "electron-squirrel-startup": "^1.0.0",
    "update-electron-app-notification": "0.0.0-development"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "electron": "12.0.0"
  }
}
